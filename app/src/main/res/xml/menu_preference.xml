<?xml version="1.0" encoding="utf-8"?>
<PreferenceGroup xmlns:camera="http://schemas.android.com/apk/res-auto">
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:entries="@array/menu_camera_id_list"
        camera:entryIcons="@array/menu_switch_icon_list"
        camera:entryValues="@array/menu_camera_id_list"
        camera:icon="@drawable/ic_vector_switch_to_front"
        camera:key="pref_switch_camera" />
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:entries="@array/menu_flash_entries"
        camera:entryIcons="@array/menu_flash_icon_list"
        camera:entryValues="@array/menu_flash_entries_value"
        camera:icon="@drawable/ic_vector_flash_on"
        camera:key="pref_flash_mode" />
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:icon="@drawable/ic_center_focus_strong_black_24dp"
        camera:key="pref_camera_zoom" />
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:icon="@drawable/ic_center_focus_weak_black_24dp"
        camera:key="pref_focus_lens" />
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:icon="@drawable/ic_brightness_6_black_24dp"
        camera:key="pref_brightness" />
    <com.xuqiqiang.camera2.app.ui.menu.adapter.CamMenuPreference
        camera:icon="@drawable/ic_filter_black_24dp"
        camera:key="pref_filter" />
</PreferenceGroup>